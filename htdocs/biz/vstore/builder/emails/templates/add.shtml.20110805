<!-- SETUP_TAB -->

<br>

<center>
<table width=600 border=0 cellspacing=0 cellpadding=0 class="zoovytable">
<tr>
	<td class="zoovytableheader"> &nbsp; Add New Messages:</td>
</tr>
<tr>
<td>
	<form method="POST" action="index.cgi">
	<input type="hidden" name="VERB" value="MSGSAVE">
	<input type="hidden" name="NS" value="<!-- NS -->">
	<table width="100%" cellspacing=5>
	<tr>
		<td valign='top' width='100px'><b>Type:</b></td>
		<td>
		<input type="radio" value="ORDER" name="MSGTYPE"> Order<br>
		<input type="radio" value="ACCOUNT" name="MSGTYPE"> Customer<br>
		<input type="radio" value="INCOMPLETE" name="MSGTYPE"> Incomplete Item<br>
		<input type="radio" value="PRODUCT" name="MSGTYPE"> Product Notification<br>
		<input type="radio" value="SUPPLY" name="MSGTYPE"> Supply Chain Notification<br>
		<input type="radio" value="TICKET" name="MSGTYPE"> CRM/Ticket<br>
		</td>
	</tr>
	<tr>
		<td><b>Message ID:</b></td>
		<td><input type="textbox" maxlength="10" size="10" name="MSGID"> <i>(Must be unique per message)</i></td>
	</tr>
	<tr>
		<td><b>Subject:</b></td>
		<td><input type="textbox" size="60" name="MSGSUBJECT"></td>
	</tr>
	<tr>
		<td colspan=2>
		<b>Body:</b><br>
		<textarea name="MSGBODY" cols="60" rows="15"></textarea>
		</td>
	</tr>
	</table>
	<center>
		<input type="submit" class="button" value=" Add Message ">
	</center>
</td>
</tr>
</table>