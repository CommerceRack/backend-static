<KITTENHTML><![CDATA[

<div class='sub_cats'>
<!-- ROW -->
<!-- CATEGORY -->
<a href="<% print($cat_url); %>"> &#187; <% print($cat_pretty); %></a>
<!-- /CATEGORY -->
<!-- /ROW -->
</div>


]]></KITTENHTML>
