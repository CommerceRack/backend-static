<ELEMENT ID="SPECLDUALCATS" SUB="WRAP_TIER2_CATS" TYPE="SPECL" OUTPUTSKIP="1" OUTPUTFILTER="!FS=C"><HTML><![CDATA[
<%

print("");

/* get the root level set of categories for the focus cat (ex:  cat.subcat.subsubcat would display the cats for .cat)
print("");
/* the only way to get the top level category is to put the entire category path into a stack and then to retrieve the content of spot 1 */
loadurp("SREF::_CWPATH");
	pull(stack=>>$PATHSTACK,format=>"textlist",delimiter=>".",p1=>"TOPCAT");
	pop(stack=>>$PATHSTACK,namespace=>"PATH");
	load($PATH.TOPCAT); 

	element(TYPE=>"SUBCAT",HTML=>$CATHTML,MODE=>"LIST",SRC=>$_,DESCENDTREE=>"0");
	pull(stack=>>$CATSTACK,format=>"textlist",delimiter=>"|",p0=>"CAT_SAFE",p1=>"CAT_PRETTY",p2=>"CAT_URL");

:TOPICLOOP();
stop(unless=>$CATSTACK);

pop(stack=>>$CATSTACK,namespace=>"CATS");
/* only print out the subcategories of the active category that we're in */
loadurp("SREF::_CWPATH"); 
pull(stack=>>$PATHSTACK,format=>"textlist",delimiter=>".",p1=>"TOPCAT");
pop(stack=>>$PATHSTACK,namespace=>"PATH");
load($PATH.TOPCAT);

:TOPCAT_SET();

	print("<div class='top_cat'><a href='");
	print($CATS.CAT_URL); 
	print("'>");
	load($CATS.CAT_PRETTY); print();
	print("</a></div>");
	
	
	
	element(TYPE=>"SUBCAT",SRC=>$CATS.CAT_SAFE,HTML=>$KITTENHTML,MODE=>"LIST");
	print();
	goto(nb=>$CATSTACK,label=>"TOPICLOOP");
	stop();
%>
]]></HTML>
