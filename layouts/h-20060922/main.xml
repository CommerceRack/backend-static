<ELEMENT ID="LIST1" SIZE="5000" MAX="8" DATA="page:prodlist" COLS="4" FORMAT="CUSTOM" PROMPT="Product List" TYPE="PRODLIST"><HTML><![CDATA[
<!-- begin product list 1 -->
<table border="0" cellpadding="0" cellspacing="0" width="100%">
<!-- ROW -->
<tr>
<!-- PRODUCT -->
	<td width="25%" align="center" class="ztable_row" valign="top">
<div style="background-image:url(<% load($zoovy:prod_thumb); default($zoovy:prod_image1); image(w=>"132",h=>"132",bg=>"ffffff",alt=>$zoovy:prod_name); print(); %>); background-repeat:no-repeat; height:132px; width:132px;">
<a href="<% load($PROD_URL); print(); %>"><img src="//static.zoovy.com/graphics/layouts/h-20060922/circle_outline_white-132x132.gif" height="132" width="132" border="0" alt="<% load($zoovy:prod_name); default(); print(); %>"></a>
</div>
<div style="padding:3px;" class="ztable_row_small">
<a href="<% load($PROD_URL); print(); %>"><% load($zoovy:prod_name); strip(length=>"30",breaks=>"1",html=>"1"); print(); %></a>
</div>
</td>	
<!-- /PRODUCT -->
<!-- BLANK -->
	<td></td>
<!-- /BLANK -->
</tr>
<!-- /ROW -->
</table><!-- end product list 1 -->
]]></HTML>
