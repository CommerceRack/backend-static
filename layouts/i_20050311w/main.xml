<ELEMENT ID="WELCOME" MAXLENGTH="45" EDITOR="INLINE" DATA="page:email_header" PROMPT="Header" HELPER="Remember to keep this text short and to the point." DEFAULT="Featured Item" TYPE="TEXTBOX" FONT="0"><POSTTEXT><![CDATA[</span></td></tr></table><br>]]></POSTTEXT>
