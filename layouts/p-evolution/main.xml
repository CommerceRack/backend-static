.base_shadow {width:281px;height:5px;padding:5px; background-color:#555555;opacity:.15;filter: alpha(opacity=15); -moz-opacity: 0.15; font-size:1px;}
.taf_window {width:290px;height:250px;padding:5px;border:1px solid #999999; position:relative; background-color:#FFFFFF; box-sizing: border-box; -moz-box-sizing: border-box;}


/* This is for the content under the link bar. Product content is loaded into the prod_content div.*/
.prod_content		{height:350px; overflow:scroll; }

/* This controls the product name as it appears just below the link bar inside the details section. */
#prod_name			{font-size:14px; font-weight:bold;}

/* this is the cell that contains the tabs and their content */
.prod_contentCol	{padding-right:3px; padding-left:3px;}


/* this is the class for the UL that contains the tab list.  Set indentation with a left margin. */
.tabset_tabs {margin:0px; padding:0px;}
/* controls the appearance of the tab bar and the tabs themselves */
div#tabs {float: left; font-size: 8pt;} 


div#tabs li {float: left; margin:0px 2px 0px 0px; border:1px solid #666666; border-bottom:0px; width:auto; padding:0px; list-style-type:none;}
div#tabs a {display: block;  padding:4px 10px 4px 10px; text-decoration: none;  color: #6b654f; } 
 
/* this is for the content that is revealed by the tabs */
div#tab_content {color: #666666;  width: 550px;  padding:5px; line-height:140%;}
.prod_content	{border:1px solid #666666; float:left; background-color:#FFFFFF; font-size:10pt; color:#333333;}

div#pop_window	{border:1px solid #000000; height:350px; width:600px; overflow:scroll; background-color:#ffffff;}
.anylinkcss{position:absolute; visibility: hidden; font:normal 11px;line-height: 18px;z-index: 1000;width: 205px;margin-left:85px;}
.anylinkcss a{ width: 100%; display: block; text-indent: 3px; padding: 1px 0;text-decoration: none;font-weight: bold;text-indent: 5px;}
 /*hover background color*/
.anylinkcss a:hover{background-color: #666666;color: #ffffff;}
