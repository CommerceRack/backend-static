/* if the msrp is blank or the msrp < price, do not display savings */
loadurp("product:zoovy:prod_msrp");
stop(unless=>$_);
	set($msrp=>$_);
	load($msrp);
	math(op=>"subtract",var=>$price);
	goto(label=>"END",lt=>"0");
	set($savings=>$_);

/* we only get here if msrp > price. display msrp and savings */
load($msrp);
format(money);
format(pretext=>"<div>Retail&nbsp;Price:&nbsp;<span id='prod_topData_msrp'>",posttext=>"</span></div>");
print();

load($savings);
/* don't show a savings of 0 */
stop(unless=>$_);
format(money);
format(pretext=>"<div id='prod_topData_savings'>A&nbsp;Savings&nbsp;of:&nbsp;",posttext=>"</div>"); 
print();

:END();

print("");
%>

