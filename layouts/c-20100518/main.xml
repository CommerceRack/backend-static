<ELEMENT ID="IMAGES" SUB="PAGE_PICS" TYPE="SPECL"><HTML><![CDATA[
<%
print("");

set($i=>"0");

:LOOP();

/* Need to loop through all 50 pics and checks/goto's occur in various locations, so incrementing is done at the top to avoid an infinite loop */
load($i);
math(op=>"add",var=>"1");
set($i=>$_);

load($i);
goto(label=>"END",gt=>"50");

format(pretext=>"page:picture");
loadurp($_);

/* if the current load has no value, then try the next. we have a finite number in the loop (50) */
goto(label=>"LOOP",eq=>"");

/* the filename is used in multiple places, so it is set to a var instead of loadurp'ing it several times. */
	set($image=>$_);
	load($i); format(pretext=>"<!-- ",posttext=>" -->"); print();
	print("
<div style='float:left; margin-bottom:5px; margin-right:5px; padding:1px;' class='zborder'><a href='");
	load($image);
	image(h=>"",w=>"",bg=>"ffffff",tag=>"0",m=>"0"); print();
	print("' class='MagicThumb'>");
	load($image);
	image(src=>$image,w=>"100",h=>"100",bg=>"ffffff",m=>"0",tag=>"1",alt=>$image);
	print();
	print("</a></div>");



/* Go to loop and do it all again. */
	goto(label=>"LOOP",ne=>"");

:END();
	print("<!-- the end is nigh! -->");
	stop();
%>
]]></HTML>
